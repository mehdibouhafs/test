# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~



GET     /home              @controllers.Application.home

GET     /                       controllers.UploadController.index()

# An example controller showing a sample home page
POST     /upload                   controllers.UploadController.upload()

POST    /getCols                      controllers.UploadController.getCols()

POST    /validate                       controllers.UploadController.validate()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# Home page
#GET     /index                          controllers.HomeController.getIndex(id: Long ?= 0)
#POST    /index                          controllers.HomeController.postIndex()


